{
  "webserver":{
    "port":8080,
    "statics":"www"
  },
  "languageBundleDir":"<BUNDLE_DIR>",
  "eventBusBridge":{
    "inbound":[
      {
        "address":"appsist:service:iid:server"
      },
      {
        "address_re":"appsist:service:iid:server:.+"
      }
    ],
    "outbound":[
      {
        "address_re":"appsist:service:iid:client:.+"
      }
    ]
  },
  "commonConfig":{
    "statusSignal":{
      "groupId":"de.appsist.service",
      "interval":60000,
      "channel":"appsist:service:status"
    }
  },
  "deploys":[
    {
      "id":"io.vertx~mod-mongo-persistor~2.1.0",
      "config":{
        "address":"vertx.mongopersistor",
        "host":"localhost",
        "port":27017,
        "pool_size":10,
        "db_name":"festo"
      }
    }
  ],
  "services":{
    "myservice":{
      "version":"1.0.0",
      "config":{
        "foo":"bar"
      }
    }
  }
}